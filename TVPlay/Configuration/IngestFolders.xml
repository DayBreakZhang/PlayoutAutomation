<?xml version="1.0" encoding="utf-8"?>
<IngestDirectories xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <IngestDirectory>
    <IsRecursive>true</IsRecursive>
    <MediaLoudnessCheckAfterIngest>true</MediaLoudnessCheckAfterIngest>
    <DirectoryName>Ingest</DirectoryName>
    <Folder>d:\Ingest</Folder>
    <EncodeParams>-preset veryfast -pix_fmt yuv420p -g 30</EncodeParams>
    <Extensions />
    <VideoCodec>libx264</VideoCodec>
    <AudioCodec>aac</AudioCodec>
  </IngestDirectory>
</IngestDirectories>